{
  "schemaVersion": 1,
  "id": "clickmobs",
  "version": "${mod_version}",
  "name": "ClickMobs",
  "description": "A simple plugin that allows you to pick up any mob into your inventory.",
  "authors": [
    "Clickism"
  ],
  "contact": {
    "homepage": "https://modrinth.com/plugin/clickmobs",
    "sources": "https://github.com/Clickism/ClickMobs"
  },
  "license": "ARR",
  "icon": "assets/clickmobs/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.clickism.clickmobs.platform.fabric.FabricEntrypoint"
    ]
  },
  "mixins": [
    "clickmobs.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}